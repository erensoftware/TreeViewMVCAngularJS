@using TreeViewMVCAngularJS.Entity
@model IEnumerable<Firm>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Firm List</h2>

<div class="table-responsive">
    <table class="table table-hover">
        <thead class="thead-light">
            <tr>
                <th>@Html.DisplayNameFor(t => t.FirmId)</th>
                <th>@Html.DisplayNameFor(t => t.FirmName)</th>
                <th>@Html.DisplayNameFor(t => t.IsActive)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Firm item in Model)
            {
                <tr>
                    <td>@item.FirmId.ToString()</td>
                    <td>@item.FirmName</td>
                    <td>@item.IsActive.ToString()</td>
                </tr>
            }
        </tbody>
    </table>
</div>
